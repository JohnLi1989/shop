<% include layout %>
<link rel="stylesheet" href="/stylesheets/tishi.css">
<body style="padding-bottom: 0px;">
<style type="text/css">
    .qq_menu_layer{position:fixed;top:0;left:0;bottom:0;right:0;background-color:rgba(0,0,0,0.5);z-index:950}.qq_menu{position:fixed;top:0;left:0;right:0;background-color:#FFF}.qq_menu:after{content:"\020";height:1px;width:100%;display:block;background-color:#B2B2B2;position:absolute;bottom:0;left:0}.qq_menu ul{margin:0;list-style:none;-webkit-box-sizing:border-box;box-sizing:border-box;padding:0 4px 0 0}.qq_menu ul:after{clear:both;content:'\20';display:block}.qq_menu ul li{margin:0;padding:0;width:20%;height:63px;float:left;border-right:solid 1px #ddd;border-bottom:solid 1px #ddd;text-align:center;background-color:white}.qq_menu ul li:nth-child(5n){margin-right:-5px}.qq_menu ul li a{display:block;padding-top:1px;text-decoration:none;color:#333}.qq_menu ul li i{margin:10px auto 0}.qq_menu ul li span{display:block;height:20px;line-height:20px;font-size:12px;color:#000}.qq_menu .icon_qq,.qq_menu .icon_qzone,.qq_menu .icon_copy,.qq_menu .icon_close,.qq_menu .icon_home,.qq_menu .icon_search,.qq_menu .icon_profile,.qq_menu .icon_star,.qq_menu .icon_history,.qq_menu .icon_cart,.qq_menu .icon_vip,.qq_menu .qq_menu_v2 .icon_share,.qq_menu_v2 .qq_menu .icon_share{width:30px;height:30px;display:block;background:transparent url(http://pics3.paipaiimg.com/update/20140923/index_qq_menu_icons_v3.png) no-repeat center center;background-size:150px 90px}.qq_menu .icon_qq{background-position:0 0}.qq_menu .icon_qzone{background-position:-30px 0}.qq_menu .icon_copy{background-position:-60px 0}.qq_menu .icon_close{background-position:-90px 0}.qq_menu .icon_home{background-position:0 -31px}.qq_menu .icon_search{background-position:-30px -31px}.qq_menu .icon_profile{background-position:-60px -31px}.qq_menu .icon_star{background-position:-90px -31px}.qq_menu .icon_history{background-position:-120px -31px}.qq_menu .icon_cart{background-position:-120px 0px}.qq_menu .icon_vip{background-position:0 -60px}.qq_menu_layer_v2{position:fixed;top:0;left:0;bottom:0;right:0;background-color:transparent;z-index:950;font-family:Helvetica,STHeiti STXihei, Microsoft JhengHei, Microsoft YaHei, Arial}.qq_menu_v2{position:fixed;top:0;right:0;width:140px;background-color:#FFF;-moz-box-shadow:0px 2px 8px rgba(0,0,0,0.25);-webkit-box-shadow:0px 2px 8px rgba(0,0,0,0.25);box-shadow:0px 2px 8px rgba(0,0,0,0.25)}.qq_menu_v2 ul{margin:0;padding:0;width:100%;list-style:none;-webkit-box-sizing:border-box;box-sizing:border-box}.qq_menu_v2 ul:after{clear:both;content:'\20';display:block}.qq_menu_v2 ul li{margin:0;padding:0;height:44px;line-height:44px;border-bottom:solid 1px #dedfe0;text-align:left;background-color:white;position:relative}.qq_menu_v2 ul li:last-child{border-bottom:0 none}.qq_menu_v2 ul li:active{background-color:#dedfe0}.qq_menu_v2 ul li a{text-decoration:none}.qq_menu_v2 ul li span{display:block;color:#000;padding-left:56px}.qq_menu_v2 ul li i{position:absolute;left:15px;top:9px}.qq_menu_v2 ul li span{font-size:15px}.qq_menu_v2 .qq_menu .icon_qq,.qq_menu .qq_menu_v2 .icon_qq,.qq_menu_v2 .qq_menu .icon_qzone,.qq_menu .qq_menu_v2 .icon_qzone,.qq_menu_v2 .qq_menu .icon_copy,.qq_menu .qq_menu_v2 .icon_copy,.qq_menu_v2 .qq_menu .icon_close,.qq_menu .qq_menu_v2 .icon_close,.qq_menu_v2 .qq_menu .icon_vip,.qq_menu .qq_menu_v2 .icon_vip,.qq_menu_v2 .icon_star,.qq_menu_v2 .icon_share,.qq_menu_v2 .icon_profile,.qq_menu_v2 .icon_cart,.qq_menu_v2 .icon_home,.qq_menu_v2 .icon_search,.qq_menu_v2 .icon_history{width:23px;height:23px;display:inline-block;background:transparent url(http://static.paipaiimg.com/qq/img/mall/qq_menu_icons_v4.png?t=1415698733907) no-repeat center center;background-size:150px 50px}.qq_menu_v2 .icon_star{background-position:0 1px}.qq_menu_v2 .icon_share{background-position:-23px 0}.qq_menu_v2 .icon_profile{background-position:-46px 1px}.qq_menu_v2 .icon_cart{background-position:-69px 0}.qq_menu_v2 .icon_home{background-position:-92px 0}.qq_menu_v2 .icon_search{background-position:-115px 0}.qq_menu_v2 .icon_history{background-position:0 -24px}.qq_footer{margin:45px 0}.qq_footer .jd_logo{height:15px;width:86px;margin:15px auto 0 auto;background:url(http://static.paipaiimg.com/wx/img/jd_logo_min.png);background-size:cover;-webkit-background-size:cover}.qq_footer .qq_info{font-size:12px;text-align:center;color:#666}
</style>

<div id="content" style="width:100%;">
    <div class="wx_wrap" id="wrapper">
        <div id="editContent" class="address_bar_wrap fixed">
            <div class="address_bar">
                <span class="btn" id="editBtn">编辑</span>
            </div>
        </div>
        <div id="listContent" style="position: absolute; width: 100%; top: -35px; transform: translateY(0px);">
            <div id="list">
                <div class="cart_refresh" id="pullDown">
                    <div class="icon"><i id="pullDownIC" class=""></i></div>
                    <p id="pullDownLabel">下拉刷新购物车</p>
                </div>
                <div class="section"></div>
            </div>
        </div>
        <div id="emptyContent">
            <div class="shopcart_none" id="normalEmpty" style="display:none;position:relative;padding-top:1px">
                <i class="icon"></i>
                <p class="title">您的购物车内还没有商品！</p>
                <div class="mod_btns">
                    <a href="/shop/users/me.do" class="mod_btn">个人中心</a>
                    <a href="/shop/goods/promotion.do" class="mod_btn" id="rego">立即逛逛</a>
                </div>
            </div>
        </div>
    </div>
    <div id="mainViewFoot">
        <p class="fixBar_tips" style="display: none;" id="outofstockTip">部分商品在所选地区缺货，请取消勾选后结算</p>

        <div class="fixBar" name="checkgroup" id="checkAllBtn">
            <i class="icon_select" id="chooseAll">全选</i>
            <div class="total" id="totalConfirmDiv">
                <p>总计：<strong>¥<span id="totalPrice">0.0</span></strong><small>(不含运费)</small></p>
                <a href="javascript:;" class="btn_pay" id="shopCartConfirm">结算（<em id="totalNum">0</em>）</a>
            </div>
            <div class="btns" style="display:none" id="operateDiv">
                <a href="javascript:;" class="btn_2" id="addFavor">移至收藏</a>
                <a href="javascript:;" class="btn_3" id="deleteBtn">删除</a>
            </div>
        </div>

    </div>

    <!-- ajax loading -->
    <div class="mod_alert mod_alert_loading fixed" id="wxloading">
        <div class="wx_loading2">
            <i class="wx_loading_icon"></i>
        </div>
    </div>
    <!-- ajax loading end-->

    <div id="dialogConent" class="mod_alert fixed">
        <i class="icon"></i>
        <p> 是否确认将已选中商品删除？</p><p class="btns">
            <a href="javascript:;" class="btn btn_1" id="confirm">确认</a>
            <a href="javascript:;" class="btn" id="cancel">取消</a>
        </p>
    </div>
    <div id="dialogConent2" class="mod_alert fixed">
        <i class="icon"></i>
        <p> 是否确认将已选中商品移至收藏？</p><p class="btns">
            <a href="javascript:;" class="btn btn_1" id="confirm2">确认</a>
            <a href="javascript:;" class="btn" id="cancel2">取消</a>
        </p>
    </div>
</div>
<div class="mask"></div>
</div>
<script src="/javascripts/shopcart"></script>
<script src="/javascripts/quickMenu.js"></script>
</body>